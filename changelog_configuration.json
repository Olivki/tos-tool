{
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "exclude_merge_branches": [],
  "sort": {
    "order": "ASC",
    "on_property": "mergedAt"
  },
  "template": "${{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n</details>",
  "pr_template": "- #${{NUMBER}} ${{TITLE}}",
  "empty_template": "- no changes",
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": [
        "feature"
      ]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": [
        "fix"
      ]
    },
    {
      "title": "## 🖊️️ Changes",
      "labels": [
        "change"
      ]
    },
    {
      "title": "## 📦 Dependencies",
      "labels": [
        "dependencies"
      ]
    },
    {
      "title": "## 🧪 Tests",
      "labels": [
        "test"
      ]
    }
  ],
  "ignore_labels": [
    "ignore"
  ],
  "label_extractor": [],
  "transformers": [],
  "tag_resolver": {
    "method": "semver"
  },
  "base_branches": [],
  "custom_placeholders": [],
  "trim_values": false
}